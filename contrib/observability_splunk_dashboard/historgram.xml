      </chart>
    </panel>
    <panel>
      <title>CommitProxy Histogram - ToTLog</title>
      <chart>
        <title>CommitProxy to TLog</title>
        <search>
          <query>index=iffdb LogGroup=playstation_perffdbserver_s01 Type=Histogram  host=* Machine=* Group="ToTlog_*" | foreach LessThan* [eval newfield=&lt;&lt;FIELD&gt;&gt;] | timechart avg(LessThan*) as LessThan*</query>
          <earliest>$TimeSpan.earliest$</earliest>
