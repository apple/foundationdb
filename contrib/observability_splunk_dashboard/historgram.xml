          <query>index=$Index$ LogGroup=$LogGroup$ Type=Histogram  host=* Machine=* Group="CommitProxy" Op="GetCommitVersion" | foreach LessThan* [eval newfield=&lt;&lt;FIELD&gt;&gt;] | timechart avg(LessThan*) as LessThan*</query>
